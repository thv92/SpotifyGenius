{
  "name": "SpotifyGenius",
  "version": "1.0.0",
  "description": "Spotify + Genius Lyrics",
  "main": "index.js",
  "repository": "https://github.com/thv92/SpotifyGenius.git",
  "author": "Thien Vo <vohuythien@utexas.edu>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "buildDev": "rm -rf ./dist && webpack --mode development --config webpack.config.dev.js",
    "buildDevWatch": "rm -rf ./dist && webpack --watch --mode development --config webpack.config.dev.js",
    "startServerWDS": "webpack-dev-server --config webpack.config.dev.js",
    "startServerDev": "nodemon ./src/server/server.dev.js",
    "build": "rm -rf ./dist && webpack --env.API_URL=$API_URL --mode production --config webpack.config.prod.js",
    "startServer": "node ./src/server/server.prod.js",
    "start": "yarn build && yarn startServer"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "cors": "^2.8.5",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "query-string": "^6.2.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-svg-loader": "^2.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "yarn": "^1.13.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "engines": {
    "node": "11.4.0",
    "yarn": "1.13.0"
  },
  "devDependencies": {
    "dotenv": "^6.2.0",
    "nodemon": "^1.18.9",
    "webpack-hot-middleware": "^2.24.3"
  }
}
